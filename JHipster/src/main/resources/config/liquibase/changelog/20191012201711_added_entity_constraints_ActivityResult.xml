<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity ActivityResult.
    -->
    <changeSet id="20191012201711-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="result_unit_id"
                                 baseTableName="activity_result"
                                 constraintName="fk_activity_result_result_unit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="unit"/>

        <addForeignKeyConstraint baseColumnNames="activity_id"
                                 baseTableName="activity_result"
                                 constraintName="fk_activity_result_activity_id"
                                 referencedColumnNames="id"
                                 referencedTableName="activity"/>

    </changeSet>
</databaseChangeLog>
