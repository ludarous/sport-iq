<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity BodyCharacteristics.
    -->
    <changeSet id="20201029220412-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="height_unit_id"
                                 baseTableName="body_characteristics"
                                 constraintName="fk_body_characteristics_height_unit_id"
